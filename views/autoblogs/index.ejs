<%- include('../partials/header') %>

<h1>Daily Driven</h1>
    <table>
        <thead>
            <tr>
                <th>Make</th>
                <th>Model</th>
                <th>Engine</th>
            </tr>
        </thead>
        <tbody>
            <% autoblogs.forEach(function(a) { %>
                <tr>
                    <td><%= a.make %></td>
                    <td><%= a.model %></td>
                    <td><%= a.engine %></td>
                    <td><a href="/autoblogs/<%= a._id %>">DETAILS</a></td>
                </tr>

            <% }); %>
        </tbody>
    </table>

<%- include('../partials/footer') %>